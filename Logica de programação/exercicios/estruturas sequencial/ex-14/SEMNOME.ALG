Algoritmo "semnome"

Var

         minutos, valor : inteiro

Inicio



      escreva("Digite a quantidade de minutos: ")
      leia(minutos)

        se minutos > 100 entao
        valor <- 50 + ((minutos - 100)*2)
        senao
        valor <- 50
        fimse
        escreva("Valor a pagar: ",valor)



Fimalgoritmo