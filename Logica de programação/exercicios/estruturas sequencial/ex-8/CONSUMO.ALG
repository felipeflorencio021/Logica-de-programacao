Algoritmo "CONSUMO"

Var

      distancia, gasto, consumo : real

Inicio

               escreva("Distancia percorrida: ")
               leia(distancia)
               escreva("Combustível gasto: ")
               leia(gasto)
               consumo <- distancia/gasto
               escreva("Consumo medio = ", consumo)


Fimalgoritmo