Algoritmo "semnome"

Var

 nome: caractere
 valor, pagAmento : real
 horas: inteiro

Inicio



      escreva("Nome: ")
      leia(nome)
      escreva("Valor por hora: ")
      leia(valor)
      escreva("Horas trabalhadas: ")
      leia(horas)
      pagamento <- valor * horas
      escreva("O pagamento para ",nome," deve ser ",pagamento)






Fimalgoritmo